<!DOCTYPE html>
<html lang="en">
  <head>
    <title>TinyApp</title>
  </head>
  <body>
    <header>
      <% include ./partials/_header %>
    </header>
    <ul>
      <% for (var url in urls) { %>
        <li>
          <form action="/urls/<%= url %>/delete" method="POST">
            <a href="<%= urls[url] %>"><%= urls[url] %></a>   -   <%= url %>
            <input id="<%= url %>" type="submit" value="Delete">
          </form>
        </li>
      <% } %>
    </ul>
  </body>
</html>